<%- include('header') %>

    <table class="table table-dark table-striped">
        <thead>
            <th scope="col">Email</th>
            <th scope="col">Hobbies</th>
            <th scope="col">How often do you practise hobbies</th>
            <th scope="col">Amount of excercise</th>
            <th scope="col">Motivation/Demotivation</th>
        </thead>
        <tbody>
            <% data.forEach(doc => { %>
                <tr>
                    <td>
                        <%= doc.response.email %>
                    </td>
                    <td>
                        <%= doc.response['hobbies'] %> 
                    </td>
                    <td>
                        <%= doc.response['howoften'] %>
                    </td>
                    <td>
                        <%= doc.response['amontOfEx'] %>
                    </td>
                    <td>
                        <%= doc.response['motivation'] %> 
                    </td>
                </tr>
                <% }); %>
        </tbody>
    </table>
    </body>

    </html>